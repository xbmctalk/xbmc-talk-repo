<addon id="plugin.video.newawesomedl"
       name="AWEsomeDL"
       version="2.0.9"
       provider-name="xbmctalk.us">
  <requires>
        <import addon="xbmc.python" version="2.0"/>
        <import addon="script.module.urlresolver" version="1.0.3"/>
        <import addon="script.module.t0mm0.common" version="1.1.0"/>
		<import addon="script.module.metahandler" version="1.0.1"/>
		<import addon="script.module.beautifulsoup"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
            library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">AWEsomeDL</summary>
    <description lang="en">AWEsomeDL has been cleaned, all instances of tracking code and advertising are now gone. New AWEsomeDL. Credits go to Jas0npc as the original author, Vin, Mikey1234,Eldorado, UmOuch,MScreations, and [B]ANILKUJ[/B] for creating this addon.</description>
    <platform>all</platform>
  </extension>
</addon>

